
<h1 class="h3 mb-3 text-gray-800">Boys avarage by age</h1>

<table class="table table-striped table-hover table-sm table-responsive-xl "  width="100%" cellspacing="0">
		  <thead class="thead-light">
		    <tr>
		    	<th>Exercise/Age</th>
		    	<% (10..19).each do |n| %>
		      	<th><%= n %></th> 
		    	<% end %>
		    	
		    </tr>
		  </thead>
		  <tbody>
		  		<% num = 0 %>
		  		<% @boys.each do |s| %>
			  		<tr>
			  		<td><b><%= @execs[num].to_s.capitalize %></b></td>
			  		
			  			<% s.each do |n| %>
			  				<td><%= n %></td>
			  			<% end %>
			  		
			  		</tr>
			  		<% num = num + 1 %>
		  		<% end %>
		  </tbody>
		</table>

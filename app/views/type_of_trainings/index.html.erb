<p id="notice"><%= notice %></p>

<h1>Type Of Trainings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_of_trainings.each do |type_of_training| %>
      <tr>
        <td><%= type_of_training.title %></td>
        <td><%= link_to 'Show', type_of_training %></td>
        <td><%= link_to 'Edit', edit_type_of_training_path(type_of_training) %></td>
        <td><%= link_to 'Destroy', type_of_training, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Of Training', new_type_of_training_path %>

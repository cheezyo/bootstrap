<%= form_with(model: feedback, local: true) do |form| %>
  <% if feedback.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(feedback.errors.count, "error") %> prohibited this feedback from being saved:</h2>

      <ul>
        <% feedback.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
   
    <%= form.collection_select :player_id, Player.all, :id, :name, :class => "form-control form-control-user", :include_blank => 'choose player' %>
  </div>

  <div class="field">
    <%= form.label :feed_back %>
    <%= form.text_area :feed_back %>
  </div>

  <div class="field">
    <%= form.label :email_date %>
    <%= form.date_select :email_date %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

<p id="notice"><%= notice %></p>

<h1>Type Of Exercises</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_of_exercises.each do |type_of_exercise| %>
      <tr>
        <td><%= type_of_exercise.title %></td>
        <td><%= link_to 'Show', type_of_exercise %></td>
        <td><%= link_to 'Edit', edit_type_of_exercise_path(type_of_exercise) %></td>
        <td><%= link_to 'Destroy', type_of_exercise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Of Exercise', new_type_of_exercise_path %>

  <table class="table table-striped table-hover table-sm"id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      
                      <th>Player</th>
                      <th>Tennis m t</th>
                      <th>Tennis u t</th>
                      <th>Tennis tot mnd</th>
                      <th>Fys m t</th>
                      <th>Fys u t</th>
                      <th>Fys tot mnd</th>
                      <th>Tr. tot mnd</th>
                      <th>Tennis m t</th>
                      <th>Tennis u t</th>
                      <th>Tennis tot år</th>
                      <th>Fys m t</th>
                      <th>Fys u t</th>
                      <th>Fys tot år</th>
                      <th>Tr. tot år</th>
                      
                      
                
                    </tr>
                  </thead>
                 
                  <tbody>
                    <% @players.each do |p| %>
                    <% if p.got_user? %>
                  <tr>
                    
                    <td>
                      <%= link_to p.fullname, p  %> (<%= p.get_age %>)
                    </td>
                  <td>
                    <%= p.get_tennis_trainings(true, false, @date) / 60 %>
                  </td>
                  <td>
                    <%= p.get_tennis_trainings(false, false, @date) / 60 %>
                  </td>
                  <td><b>
                    <%= tennis = ( p.tennis_total(false, @date) / 60) %>
                  </b></td>
                  <td>
                    <%= p.get_physical_trainings(true, false, @date) / 60 %>
                  </td>
                   <td>
                    <%= p.get_physical_trainings(false, false, @date) / 60 %>
                  </td>
                   <td><b>
                    <%= physical = ( p.physical_total(false, @date) / 60 ) %>
                  </b></td>
                  <td><b>
                    <%= (tennis + physical) / 60 %>
                  </b></td>
                  <td>
                    <%= p.get_tennis_trainings(true, true, @date) / 60 %>
                  </td>
                  <td>
                    <%= p.get_tennis_trainings(false, true, @date) / 60 %>
                  </td>
                  <td><b>
                    <%= tennis_year = (p.tennis_total(false, @date) / 60) %>
                  </b></td>
                  <td>
                    <%= p.get_physical_trainings(true, true, @date) / 60 %>
                  </td>
                   <td>
                    <%= p.get_physical_trainings(false, true, @date) / 60 %>
                  </td>
                   <td><b>
                    <%= physical_year = (p.physical_total(true, @date) /60) %>
                  </b></td>
                  <td><b>
                    <%= (tennis_year + physical_year) / 60%>
                  </b></td>
                  
                </tr>
                <% end %>
                <% end %>
              </tbody>
            </table>